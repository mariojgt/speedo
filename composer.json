{
    "name": "mariojgt/speedo",
    "type": "project",
    "description": "Speedo Framework.",
    "keywords": ["framework", "speedo"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.52",
        "illuminate/view": "^8.52",
        "swiftmailer/swiftmailer": "6.0",
        "symfony/error-handler": "5.3",
        "symfony/http-foundation": "^5.1.4",
        "symfony/http-kernel": "^5.1.4",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Speedo\\": "src/"
        }
    },
    "extra": {},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
